{{ form_start(form) }}
    <div class="row">
        <div class="col-3">
            {{ form_row(form.langue) }}
        </div>
        <div class="col-3">
            {{ form_row(form.nom) }}
        </div>
        <div class="col-3">
            {{ form_row(form.prenom) }}
        </div>
        <div class="col-3">
            {{ form_row(form.telephone) }}
        </div>
    </div>
    <div class="row">
        <div class="col-3">
            {{ form_row(form.position) }}
        </div>
        <div class="col-3">
            {{ form_row(form.assurance) }}
        </div>
        <div class="col-3">
            {{ form_row(form.vols) }}
        </div>
    </div>
    <div class="row">
        <div class="col-3">
            {{ form_row(form.user) }}
        </div>
        <div class="col-3">
            {{ form_row(form.date) }}
        </div>
        <div class="col-3">
            {{ form_row(form.reservation) }}
        </div>
    </div>
    <button class="btn btn-primary">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}
