{% extends 'admin/base.html.twig' %}

{% block title %}Lang{% endblock %}

{% block body %}
<div class="container">
        <div class="row">
            <div class="col-md-12 col-sm-12" data-url="">
                <h1>Lang</h1>
                <table class="table">
                    <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ lang.id }}</td>
                        </tr>
                        <tr>
                            <th>Name</th>
                            <td>{{ lang.name }}</td>
                        </tr>
                        <tr>
                            <th>Iso_code</th>
                            <td>{{ lang.isoCode }}</td>
                        </tr>
                        <tr>
                            <th>Charset</th>
                            <td>{{ lang.charset }}</td>
                        </tr>
                    </tbody>
                </table>

                <a href="{{ path('lang_index') }}">back to list</a>

                <a href="{{ path('lang_edit', {'id': lang.id}) }}">edit</a>

                {{ include('admin/lang/_delete_form.html.twig') }}
            </div>
        </div>
    </div>
{% endblock %}
