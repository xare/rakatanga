{{ form_start(form) }}
    {# {{ form_widget(form) }} #}
    <div class="container">
        <div class="row">
            <div class="col-4">
                {{ form_row(form.nom) }}
            </div>
            <div class="col-4">
                {{ form_row(form.prenom) }}
             </div>
            <div class="col-4">
                {{ form_row(form.telephone) }}
            </div>
        </div>
        <div class="row">
            <div class="col-4">
                {{ form_row(form.email) }}
            </div>
            <div class="col-4">
                {{ form_row(form.position) }}
             </div>
            <div class="col-4">
                {{ form_row(form.arrhes) }}
            </div>
        </div>
        <div class="row">
             <div class="col-4">
                {{ form_row(form.solde) }}
            </div>
            <div class="col-4">
                {{ form_row(form.assurance) }}
             </div>
            <div class="col-4">
                {{ form_row(form.vols) }}
            </div>
        </div>
         <div class="row">
             <div class="col-4">
                {{ form_row(form.statut) }}
            </div>
            <div class="col-4">
                {{ form_row(form.remarque) }}
             </div>
            <div class="col-4">
                {{ form_row(form.date_ajout) }}
            </div>
        </div>
        <div class="row">
             <div class="col-4">
                {{ form_row(form.langue) }}
            </div>
            <div class="col-4">
             </div>
            <div class="col-4">
            </div>
        </div>
         <div class="row">
             <div class="col-4">
                <button 
                    class="btn btn-primary"
                    title="{{ button_label|default('Guardar') }}"
                    data-bs-toggle="tooltip" 
                    data-bs-placement="top">
                    <i class="fa fa-save"></i>
                </button>
             <div class="col-4">
             </div>
            <div class="col-4">
            </div>
        </div>
    </div>
{{ form_end(form) }}
