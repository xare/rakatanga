{% extends 'admin/base.html.twig' %}

{% block title %}Inscriptions{% endblock %}

{% block body %}
<div class="container">
    {{ include ('admin/shared/_controlband.html.twig',{'entity':'inscriptions'}) }}
    <div class="row">
        <div class="col-12">
            <h1>Inscriptions</h1>

            <table class="table">
                <tbody>
                    <tr>
                        <th>Id</th>
                        <td>{{ inscription.id }}</td>
                    </tr>
                    <tr>
                        <th>Nom</th>
                        <td>{{ inscription.nom }}</td>
                    </tr>
                    <tr>
                        <th>Prenom</th>
                        <td>{{ inscription.prenom }}</td>
                    </tr>
                    <tr>
                        <th>Telephone</th>
                        <td>{{ inscription.telephone }}</td>
                    </tr>
                    <tr>
                        <th>Email</th>
                        <td>{{ inscription.email }}</td>
                    </tr>
                    <tr>
                        <th>Position</th>
                        <td>{{ inscription.position }}</td>
                    </tr>
                    <tr>
                        <th>Arrhes</th>
                        <td>{{ inscription.arrhes }}</td>
                    </tr>
                    <tr>
                        <th>Solde</th>
                        <td>{{ inscription.solde }}</td>
                    </tr>
                    <tr>
                        <th>Assurance</th>
                        <td>{{ inscription.assurance }}</td>
                    </tr>
                    <tr>
                        <th>Vols</th>
                        <td>{{ inscription.vols }}</td>
                    </tr>
                    <tr>
                        <th>Statut</th>
                        <td>{{ inscription.statut }}</td>
                    </tr>
                    <tr>
                        <th>Remarque</th>
                        <td>{{ inscription.remarque }}</td>
                    </tr>
                    <tr>
                        <th>Date_ajout</th>
                        <td>{{ inscription.dateAjout ? inscription.dateAjout|date('Y-m-d H:i:s') : '' }}</td>
                    </tr>
                    <tr>
                        <th>Langue</th>
                        <td>{{ inscription.langue }}</td>
                    </tr>
                </tbody>
            </table>


            <a href="{{ path('inscriptions_edit', {'id': inscription.id}) }}">edit</a>

            {{ include('admin/inscriptions/_delete_form.html.twig') }}
        </div>
    </div>
</div>
{% endblock %}
